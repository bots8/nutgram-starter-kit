{
    "name": "bots8/nutgram-starter-kit",
    "type": "project",
    "description": "A starter kit for building telegram bot with Nutgram",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "nutgram/nutgram": "3.20.2",
        "symfony/cache": "6.4.2",
        "symfony/console": "6.4.2",
        "spatie/ray": "^1.41"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Console\\": "console/",
            "Library\\": "library/"
        }
    },
    "authors": [
        {
            "name": "Fikri R",
            "email": "fikrizhanking@gmail.com"
        }
    ],
    "license": "MIT",
    "require-dev": {
        "pestphp/pest": "^2.33"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
